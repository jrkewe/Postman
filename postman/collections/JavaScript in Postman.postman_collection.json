            "id": "9a557b83-c843-4f19-8a29-5a66939ed645",
            "exec": [
              "let studentName = \"John\";\r",
              "let age = 16;\r",
              "let year = 10;\r",
              "\r",
              "const student = {\r",
              "    studentName: \"John\",\r",
              "    age: 16,\r",
              "    year: 10,\r",
              "    welcome: function(){\r",
              "        console.log(\"Welcome\" + this.studentName);\r",
              "    }\r",
              "};\r",
              "//Converting JS Object into JSON format\r",
              "let studentJson = JSON.stringify(student);\r",
              "console.log(student);\r",
              "//Converting JSON Format into JS Object \r",
              "let studentObj = JSON.parse(studentJson);\r",
              "console.log(studentObj);\r",
              "//! JSON file cannot save functions, only data."
            ],
