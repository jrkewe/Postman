            "exec": [
              "let studentName = \"John\";\r",
              "let age = 16;\r",
              "let year = 10;\r",
              "\r",
              "const student = {\r",
              "    studentName: \"John\",\r",
              "    age: 16,\r",
              "    year: 10,\r",
              "    scores:{\r",
              "        math: 85,\r",
              "        English: 90,\r",
              "        Hindi: 85\r",
              "    }\r",
              "};\r",
              "\r",
              "console.log(student.studentName);\r",
              "console.log(student['age'])\r",
              "\r",
              "//Nested valuse\r",
              "console.log(student.scores.English);\r",
              "console.log(student[\"scores\"][\"Hindi\"]);\r",
              "console.log(student.scores[\"math\"]);\r",
              "console.log(student[\"scores\"].English)"
            ],
